<!DOCTYPE html>
<html>
<head>
  <title>get Volunteers</title>
  <%= stylesheet_link_tag 'normalize' %>
  <%= stylesheet_link_tag 'main' %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>
<header id="page-head"><a href="/">get Volunteers</a></header>
<main>
  <% if flash.any? %>
      <mark id="flash">
        <% flash.each do |key, message| %>
            <p class="flash-<%= key %>"><%= message %></p>
        <% end %>
      </mark>
  <% end %>
  <%= yield.html_safe %>
</main>
</body>
</html>
